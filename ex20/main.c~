#include <string.h>
#include "ft.h"

char	*ft_strdup(char *src);

int main(void)
{
	ft_putstr("\nResult of ft_strdup:\n");
	ft_putstr(ft_strdup("hello"));
	ft_putstr("\nResult of strdup:\n");
	ft_putstr(strdup("hello"));
	ft_putstr("\nResult of ft_strdup:\n");
	ft_putstr(ft_strdup("hellb"));
	ft_putstr("\nResult of strdup:\n");
	ft_putstr(strdup("hellb"));
	ft_putstr("\nResult of ft_strdup:\n");
	ft_putstr(ft_strdup("hellz"));
	ft_putstr("\nResult of strdup:\n");
	ft_putstr(strdup("hellz"));
}

