#include "ft.h"

int main(void)
{
	ft_putnbr(ft_strlen("\n1234\n"));
	ft_putnbr(ft_strlen("\n1234 6\n"));
	ft_putnbr(ft_strlen("\n1234
				6\n"));
	ft_putnbr(ft_strlen("\n1234	6\n"));
}

